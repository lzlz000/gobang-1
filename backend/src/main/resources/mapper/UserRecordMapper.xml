<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.yungyu16.gobang.dao.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.yungyu16.gobang.dao.entity.UserRecord">
        <id column="id" property="id"/>
        <result column="mobile" property="mobile"/>
        <result column="user_name" property="userName"/>
        <result column="pwd" property="pwd"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_time" property="modifyTime"/>
        <result column="remark" property="remark"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, mobile, user_name, pwd, is_deleted, create_time, modify_time, remark
    </sql>

</mapper>
